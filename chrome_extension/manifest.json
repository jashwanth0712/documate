{
  "manifest_version": 3,
  "name": "Google Authentication Extension",
  "version": "1.0",
  "permissions": [
    "identity",
    "identity.email"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "logo_white.png",
    "48": "logo_white.png",
    "128": "logo_white.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "logo_white.png"
  },
  "host_permissions": [
    "https://accounts.google.com/*"
  ]
}

